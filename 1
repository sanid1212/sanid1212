SELECT [CatalogName], [CubeName]
FROM $system.MDSchema_Cubes



SELECT [DatabaseName], [Name]
FROM $system.DBSCHEMA_CATALOGS


SELECT [CatalogName], [Name] AS [DatabaseName]
FROM $system.DBSCHEMA_CATALOGS


SELECT [CatalogName], [DatabaseName]
FROM OLAP_CATALOGS



SELECT [CatName] AS [CatalogName], [DatabaseName]
FROM CAMICat




WITH 
  MEMBER [Measures].[CurrentDate] AS Format(Now(), "yyyy-MM-dd HH:mm:ss")
  MEMBER [Measures].[CurrentUser] AS UserName()
  SET [UserList] AS NonEmpty([Security].[User].[User].Members)
SELECT 
  {[Measures].[CurrentDate], [Measures].[CurrentUser]} ON COLUMNS,
  {[UserList]} ON ROWS
FROM [PnlCube]




SELECT 
  [Measures].[Sales] ON COLUMNS,
  [Date].[Date].&[2024-03-15] ON ROWS
FROM [YourCubeName]





SELECT 
  [Measures].Members ON COLUMNS,
  [Date].[Date].&[2024-03-15] ON ROWS
FROM [YourCubeName]




SELECT 
  [Measures].Members ON COLUMNS,
  [Time].[Date].&[2024-03-15] ON ROWS
FROM [YourCubeName]



SELECT [CATALOG_NAME], [CUBE_NAME], [DIMENSION_NAME]
FROM $system.MDSchema_Dimensions





SELECT [DIMENSION_UNIQUE_NAME] ON COLUMNS
FROM $system.MDSchema_Dimensions
WHERE [CUBE_NAME] = 'YourCubeName'
