@echo off
setlocal enabledelayedexpansion

REM Path to the Chrome executable
set "chromePath=C:\Program Files\Google\Chrome\Application\chrome.exe"

REM Path to the file containing URLs
set "urlFile=urls.txt"

REM Check if Chrome path exists
if not exist "!chromePath!" (
    echo Chrome not found at: !chromePath!
    exit /b 1
)

REM Check if URL file exists
if not exist "!urlFile!" (
    echo URL file not found: !urlFile!
    exit /b 1
)

REM Open each URL in Chrome
for /f "usebackq delims=" %%A in ("!urlFile!") do (
    start "" "!chromePath!" "%%A"
)

endlocal
