Set objShell = CreateObject("WScript.Shell")
Set objExec = objShell.Exec("cmd /c dir")

strOutput = objExec.StdOut.ReadAll()
strError = objExec.StdErr.ReadAll()

If strOutput <> "" Then
    MsgBox "Directory Listing:" & vbCrLf & strOutput, 64, "DIR Command Output"
Else
    MsgBox "Error:" & vbCrLf & strError, 16, "Error"
End If
